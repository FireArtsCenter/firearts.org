{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		],
		"paths": {
			"@/components/*": ["./src/components/*"],
			"@/layouts/*": ["./src/layouts/*"],
			"@/vendors/*": ["./src/vendors/*"]
		},
		"resolveJsonModule": true
	}
}
