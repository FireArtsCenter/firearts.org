{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		],
		"paths": {
			"@/types/*": ["./src/types/*"],
			"@/components/*": ["./src/components/*"],
			"@/layouts/*": ["./src/layouts/*"],
			"@/vendors/*": ["./src/vendors/*"],
			"@/utils/*": ["./src/utils/*"],
			"@/lib/*": ["./src/lib/*"],
			"@/constants": ["./src/constants.ts"]
		},
		"resolveJsonModule": true
	}
}
